{
	"extends": "../../../../tsconfig.json",
	"compilerOptions": {
		"declaration": false,
		"noEmit": false
	},
	"tstl": {
		"noHeader": true,
		"luaLibImport": "require",
		"noImplicitSelf": true,
		"luaTarget": "5.3",
		"luaBundle": "__lua__/bundle.lua",
		"luaBundleEntry": "sample.tsx",
		"luaPlugins": [
			{
				"name": "../transformers",
				"import": "JsxTransformer"
			}
		]
	},
	"include": [
		"../../.."
	],
	"exclude": [
		"transformers"
	]
}